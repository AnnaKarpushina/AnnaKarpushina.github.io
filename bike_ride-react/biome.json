{
	"$schema": "https://biomejs.dev/schemas/1.9.3/schema.json",
	"vcs": {
	  "enabled": false,
	  "clientKind": "git",
	  "useIgnoreFile": false
	},
	"files": {
	  "ignoreUnknown": false,
	  "ignore": []
	},
	"formatter": {
	  "enabled": true,
	  "indentStyle": "tab"
	},
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true,
		"suspicious": {
		  "noDebugger": "off",
		  "noConsoleLog": "off"
		},
		"correctness": {
		  "useExhaustiveDependencies": "off"
		},
		"a11y": {
		  "useKeyWithClickEvents": "off"
		},
		"style": {
		  "useShorthandFunctionType": "off",
		  "useImportType": "off",
		  "noUselessElse": "off"
		},
		"complexity": {
		  "useOptionalChain": "off"
		}
	  }
	},
	"javascript": {
	  "formatter": {
		"quoteStyle": "single",
		"lineWidth": 100,
		"trailingCommas": "all"
	  }
	}
  }
